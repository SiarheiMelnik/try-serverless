{
  "parser"  : "babel-eslint",
  "extends" : [
    "airbnb-base"
  ],
  "rules": {
    "default-case": 0,
    "global-require": 0,
    "new-cap": [2, {"capIsNew": false, "newIsCap": true}],
    "no-confusing-arrow": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "new-parens": 0
  }
}
